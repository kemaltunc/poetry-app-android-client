    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/tab_graph"
    app:startDestination="@id/homeFragment"
    tools:ignore="UnusedNavigation">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.tunc.androidpoetryapp.presentation.ui.home.HomeFragment"
        android:label="fragment_splash"
        tools:layout="@layout/fragment_splash" >
        <action
            android:id="@+id/action_homeFragment_to_profileFragment2"
            app:destination="@id/profileFragment2" />
    </fragment>
    <fragment
        android:id="@+id/profileFragment2"
        android:name="com.tunc.androidpoetryapp.presentation.ui.profile.ProfileFragment"
        android:label="ProfileFragment" />
</navigation>